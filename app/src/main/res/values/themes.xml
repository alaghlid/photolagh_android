<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ApplicationGestionPhotos_LAGHLID" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/Blue1</item>
        <item name="colorPrimaryVariant">@color/Blue2</item>
        <item name="colorOnPrimary">@color/Blue5</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/Blue5</item>
        <item name="colorSecondaryVariant">@color/RoseVale</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:fontFamily">@font/yatra_one</item>
    </style>

    <style name="Theme.ApplicationGestionPhotos_LAGHLID.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.ApplicationGestionPhotos_LAGHLID.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.ApplicationGestionPhotos_LAGHLID.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="SplashTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">?attr/colorPrimary</item>
        <item name="colorPrimaryDark">?attr/colorPrimaryDark</item>
        <item name="colorAccent">?attr/colorAccent</item>
        <item name="android:statusBarColor">?attr/colorAccent</item>
    </style>

</resources>